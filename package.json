    "package-json-from-dist": "^1.0.1",
    "path-is-absolute": "^2.0.0",
    "path-key": "^4.0.0",
    "path-scurry": "^2.0.0",
    "process-nextick-args": "^2.0.1",
    "qs": "^6.14.0",
    "read-config-file": "^6.4.0",
    "readable-stream": "^4.7.0",
    "safe-buffer": "^5.2.1",
    "shebang-command": "^2.0.0",
    "shebang-regex": "^4.0.0",
    "side-channel": "^1.1.0",
    "side-channel-list": "^1.0.0",
    "side-channel-map": "^1.0.1",
    "signal-exit": "^4.1.0",
    "string-width": "^7.2.0",
    "string-width-cjs": "^5.1.1",
    "string_decoder": "^1.3.0",
    "strip-ansi": "^7.1.0",
    "strip-ansi-cjs": "^8.0.0",
    "util-deprecate": "^1.0.2",
    "uuid": "^11.1.0",
    "which": "^5.0.0",
    "wrap-ansi": "^9.0.0",
    "wrap-ansi-cjs": "^8.0.0",
    "wrappy": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vercel/ncc": "^0.38.3",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "electron": "25.9.8",
    "electron-builder": "^24.6.4",
    "postcss": "^8.4.27",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.3.3",
    "tsx": "^4.20.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "com.everytime.dingo-track",
    "productName": "Dingo Track",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "dist-bundle/**/*",
      "src/**/*",
      "public/**/*",
      "assets/**/*",
      ".env",
      "package.json",
      "node_modules/**/*"
    ],
    "asar": false,
    "icon": "assets/app-icon.png",
    "mac": {
      "icon": "assets/app-icon.png",
      "category": "public.app-category.productivity",
      "target": {
        "target": "dmg",
        "arch": [
          "x64",
          "arm64"
        ]
      },
      "extendInfo": {
        "LSUIElement": 1,
        "LSBackgroundOnly": false
      }
    }
  }
}